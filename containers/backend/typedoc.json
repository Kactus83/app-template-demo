{
    "entryPoints": ["src/app.barrel.ts"],
    "out": "./deployments/docs",
    "readme": "README.md", 
    "entryPointStrategy": "expand",
    "plugin": ["typedoc-material-theme"],
    "themeColor": "#cb9820",
    "navigation": {
      "includeCategories": true,
      "includeGroups": true,
      "includeFolders": true,
    },
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "includeVersion": true,
    "categorizeByGroup": true,
  }
  